{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RPC Request",
  "type": "object",
  "required": [
    "protocol",
    "type",
    "module",
    "method",
    "version",
    "data"
  ],
  "properties": {
    "protocol": {
      "type": "integer",
      "const": 1
    },
    "type": {
      "type": "string",
      "const": "response"
    },
    "module": {
      "type": "string",
      "minLength": 1
    },
    "method": {
      "type": "string",
      "minLength": 1
    },
    "version": {
      "type": "integer",
      "minimum": 1
    },
    "data": {
      "title": "RPC Data",
      "description": "Type depends on the rpc call."
    }
  }
}
